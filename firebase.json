{
    "hosting": {
      "public": "public",
  
      "ignore": [
        "firebase.json"
      ],
  
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
  
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key" : "Cache-Control",
              "value" : "max-age=604800"
            }
          ]
        }
      ]
  
    }
  }